<!DOCTYPE html>

<title>WhishList index</title>

{% block body %}
    <h1>WhishList index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for whish_list in whish_lists %}
            <tr>
                <td>{{ whish_list.id }}</td>
                <td>
                    <a href="{{ path('whish_list_show', {'id': whish_list.id}) }}">show</a>
                    <a href="{{ path('whish_list_edit', {'id': whish_list.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="2">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('whish_list_new') }}">Create new</a>
{% endblock %}
