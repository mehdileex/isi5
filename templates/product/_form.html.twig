<div class="row">
    {% form_theme form 'bootstrap_4_layout.html.twig' %}


{{ form_start(form) }}
    <div class="row">
        <div class="col-md-4"> {{ form_row(form.title) }}</div>
        <div class="col-md-4"> {{ form_row(form.rating) }}</div>
        <div class="col-md-4"> {{ form_row(form.description) }}</div>
        <div class="col-md-4"> {{ form_row(form.price) }}</div>
        <div class="col-md-4"> {{ form_row(form.thumb) }}</div>
        <div class="col-md-4"> {{ form_row(form.color) }}</div>
        <div class="col-md-4"> {{ form_row(form.qunatity) }}</div>
        <div class="col-md-4"> {{ form_row(form.likes) }}</div>
        <div class="col-md-4"> {{ form_row(form.category) }}</div>
        <div class="col-md-4"> {{ form_row(form.files) }}</div>


    </div>

    <button class="btn btn-success">{{ button_label|default('Save') }}</button>
{{ form_end(form) }}
</div>